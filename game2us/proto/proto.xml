  <types>
    <type name="CS_LOGIN_REQ">
      <item name="UserName" type="string"/>
      <item name="UserPwd" type="string"/>
    </type>
    <type name="SC_LOGIN_ACK">
      <item name="Token" type="string"/>
    </type>

    <type name="CS_REGISTER_REQ">
      <item name="UserName" type="string"/>
      <!-- 用户名 -->
      <item name="UserPwd" type="string"/>
      <!-- 密码 -->
      <item name="MacAddress" type="string"/>
      <!-- mac地址 -->
    </type>

    <type name="SC_REGISTER_ACK">
      <item name="Token" type="string"/>
    </type>

    <type name="CS_CREATE_REQ">
      <!-- 创建协议(客户端请求)在login后得到 ERROR_NEED_CREATE时才发此协议 -->
      <item name="Name" type="string"/>
      <!-- 昵称 -->
      <item name="CountryId" type="int"/>
      <!-- 国家id -->
      <item name="CaptainId" type="int"/>
      <!-- 队长id -->
    </type>
    <type name="SC_CREATE_ACK">

    </type>

    <type name="SDrop">
      <item name="Id" type="int"/>
      <item name="Param1" type="int"/>
      <item name="Param2" type="int"/>
      <item name="Param3" type="int"/>
    </type>

    <!-- 玩家属性信息 -->
    <type name="SInfo">
      <!-- Key Value结构 -->
      <item name="Type" type="int"/>
      <item name="Value" type="int"/>
    </type>

    <type name="SProperties">
      <!-- 玩家属性 -->
      <item name="Name" type="string" />
      <!-- 名字 -->
      <item name="InfoArr" type="SInfo" isArray="1"/>
      <!-- 属性数组 key 为 USER_DATA_PROPERTY_LEVEL 等-->
    </type>

    <type name="SCardInfos">
      <!-- 某个卡牌信息 -->
      <item name="InfoArr" type="SInfo" isArray="1"/>
      <!-- 属性数组 key 为 USER_DATA_CARDS_UID 等-->
    </type>

    <type name="SCardBag">
      <!-- 卡牌背包 -->
      <item name="CardArr" type="SCardInfos" isArray="1"/>
    </type>

    <type name="SPositionInfo">
      <!-- 阵型中的位置信息 -->
      <item name="Position" type="int" />
      <item name="CardUidsArr" type="int" isArray="1"/>
    </type>

    <type name="SFormation">
      <!-- 阵型信息 -->
      <item name="PositionInfoArr" type="SPositionInfo" isArray="1"/>
    </type>

    <type name="SEquipInfos">
      <!-- 某个装备信息 -->
      <item name="InfoArr" type="SInfo" isArray="1"/>
      <!-- 属性数组 key 为 USER_DATA_EQUIPS_UID 等-->
    </type>

    <type name="SEquipBag">
      <!-- 装备背包信息 -->
      <item name="EquipArr" type="SEquipInfos" isArray="1"/>
    </type>

    <type name="SFragmentInfos">
      <!-- 某个碎片信息 -->
      <item name="InfoArr" type="SInfo" isArray="1"/>
      <!-- 属性数组 key 为 碎片id -->
    </type>

    <type name="SFragmentBag">
      <!-- 碎片背包信息 -->
      <item name="FragmentArr" type="SFragmentInfos" isArray="1"/>
    </type>

    <type name="SMaterialInfos">
      <!-- 某个材料信息 -->
      <item name="InfoArr" type="SInfo" isArray="1"/>
      <!-- 属性数组 key 为 材料id-->
    </type>

    <type name="SMaterialBag">
      <!-- 材料背包信息 -->
      <item name="MaterialArr" type="SMaterialInfos" isArray="1"/>
    </type>

    <type name="SLimit">
      <!-- 每日登陆奖励信息 -->
      <item name="LoginRewardId" type="int"/>
      <!-- -->
      <item name="LoginRewardDay" type="int"/>
      <item name="LoginRewardLooted" type="int"/>
    </type>

    <type name="SChildMapInfo">
      <!-- 某个子地图信息 -->
      <item name="ChildMapId" type="int"/>
      <item name="State" type="int"/>
    </type>

    <type name="SMapInfo">
      <!-- 某个大地图信息 -->
      <item name="MapId" type="int"/>
      <item name="ChildMapArr" type="SChildMapInfo" isArray="1"/>
    </type>

    <type name="SMap">
      <!-- 某种类型地图信息 -->
      <item name="MapType" type="int"/>
      <item name="MapArr" type="SMapInfo" isArray="1"/>
    </type>

    <type name="SMapGather">
      <!-- 所有地图信息 -->
      <item name="MapGahterArr" type="SMap" isArray="1"/>
    </type>

    <type name="SUserInfo" class="union">
      <item name="Property" type="SProperties" id="USER_DATA_FIRST_PROPERTY"/>
      <!-- 属性信息 -->
      <item name="CardBag" type="SCardBag" id="USER_DATA_FIRST_CARDS"/>
      <!-- 卡牌背包信息 -->
      <item name="Formation" type="SFormation" id="USER_DATA_FIRST_FORMATION"/>
      <!-- 阵型信息 -->
      <item name="EquipBag" type="SEquipBag" id="USER_DATA_FIRST_EQUIPS"/>
      <!-- 装备背包信息 -->
      <item name="Limit" type="SLimit" id="USER_DATA_FIRST_LIMIT"/>
      <!-- 每日登陆拾取信息 -->
      <item name="FragmentBag" type="SFragmentBag" id="USER_DATA_FIRST_FRAGMENTS"/>
      <!-- 碎片信息 -->
      <item name="MaterialBag" type="SMaterialBag" id="USER_DATA_FIRST_MATERIALS"/>
      <!-- 材料信息 -->
      <item name="Maps" type="SMapGather" id="USER_DATA_FIRST_MAPS"/>
      <!-- 地图信息 -->
    </type>

    <type name="CS_GETINFO_REQ">
      <!-- 获得玩家信息(用于客户端请求) -->
      <item name="InfoType" type="int"/>
      <!-- 对应大的类型 宏定义 (如获取玩家属性 InfoType = USER_DATA_FIRST_PROPERTY)-->
    </type>
    <type name="SC_GETINFO_ACK">
      <!-- 获得玩家信息(用于服务端返回) -->
      <item name="InfoType" type="int"/>
      <!-- 对应大的类型 宏定义 (如玩家属性信息 InfoType = USER_DATA_FIRST_PROPERTY)-->
      <item name="Info" type="SUserInfo" select="InfoType"/>
